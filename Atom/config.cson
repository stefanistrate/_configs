"*":
  core:
    disabledPackages: [
      "atom-beautify"
    ]
    telemetryConsent: "no"
    themes: [
      "one-light-ui"
      "solarized-light-syntax"
    ]
    useTreeSitterParsers: false
  editor:
    fontSize: 13
    scrollPastEnd: true
  "exception-reporting":
    userId: "898a6da8-1036-4c89-81f7-06228adf326d"
  kite:
    showWelcomeNotificationOnStartup: false
  "markdown-preview":
    useGitHubStyle: true
  "python-yapf":
    yapfPath: "/usr/local/bin/yapf"
    yapfStyle: "google"
  "tree-view":
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
