<presets>
  <preset name="1240px">
    <size>1240</size>
    <method>1</method>
    <duplicateimage>1</duplicateimage>
    <converttosmartobject>1</converttosmartobject>
    <edgemask>1</edgemask>
    <saturationcompensation>0</saturationcompensation>
    <contrastcompensation>0</contrastcompensation>
    <presharpen>0</presharpen>
    <presharpenvalue>1</presharpenvalue>
    <stepratio>1.6667</stepratio>
    <usmvalue>180</usmvalue>
    <usmradius>0.9</usmradius>
    <usmthreshold>5</usmthreshold>
    <edgeblur>0.5</edgeblur>
    <layeropacity>35</layeropacity>
    <usewatermark>1</usewatermark>
    <watermarkpath>/data/portfolio/watermarks/white-7pt.psd</watermarkpath>
    <watermarkopacity>75</watermarkopacity>
    <watermarkpos>2</watermarkpos>
  </preset>
